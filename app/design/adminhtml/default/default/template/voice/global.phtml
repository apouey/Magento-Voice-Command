		'<?php echo Mage::helper('voice')->__('customer'); ?>' 		: function () { r(url['goToCustomer']); },
		'<?php echo Mage::helper('voice')->__('category'); ?>' 		: function () { r(url['goToCategory']); },
		'<?php echo Mage::helper('voice')->__('order'); ?>' 		: function () { r(url['goToOrder']); },
		'<?php echo Mage::helper('voice')->__('invoice'); ?>' 		: function () { r(url['goToInvoice']); },
		'<?php echo Mage::helper('voice')->__('shipment'); ?>' 		: function () { r(url['goToShipment']); },
		'<?php echo Mage::helper('voice')->__('creditmemo'); ?>' 	: function () { r(url['goToCreditmemo']); },
