	var globalCommands = {
		'<?php echo Mage::helper('voice')->__('dashboard'); ?>'		: function () { r(url['goToDashboard']); },
		'<?php echo Mage::helper('voice')->__('cache'); ?>'			: function () { r(url['goToCache']); },
		'<?php echo Mage::helper('voice')->__('configuration'); ?>' : function () { r(url['goToConfiguration']); },
		'<?php echo Mage::helper('voice')->__('index'); ?>' 		: function () { r(url['goToProcess']); },	
